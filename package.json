{
  "name": "kira-sekira",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build && cd dist && cp index.html 404.html",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "grid": "node smartgrid.js",
    "public": "npm run build-only && gh-pages -d dist -t --nojekyll --cname kira-sekira.ru",
    "reset": "git fetch --all && git reset --hard origin/master"
  },
  "dependencies": {
    "@jamescoyle/vue-icon": "^0.1.2",
    "@mdi/js": "^7.4.47",
    "@yeger/vue-masonry-wall": "^5.0.6",
    "pinia": "^2.1.7",
    "smooth-scroll": "^16.1.3",
    "vue": "^3.3.11",
    "vue-easy-lightbox": "^1.17.0",
    "vue-particles": "^1.0.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.49.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.17.0",
    "gh-pages": "^6.1.1",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "smart-grid": "^2.1.2",
    "stylus": "^0.62.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
